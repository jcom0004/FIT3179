{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 800,
  "height": 500,
  "title": "Number of Postgraduate Degrees in Australia",
  "data": {
     "url": "https://raw.githubusercontent.com/jcom0004/FIT3179/main/aus_state.json",
     "format": {"type": "topojson", "feature": "aus_state"}
      },
      "transform": [{
          "lookup": "properties.STATE_NAME",
          "from": {
            "data": {
              "url": "https://raw.githubusercontent.com/jcom0004/FIT3179/main/Number%20of%20post%20grad%20degrees%20by%20state.csv"
        },
        "key": "State",
        "fields": ["Degrees"]
          }
       },
        {"calculate": "datum.Degrees", "as": "Thousands of Degrees"} 
    ],
  "projection": {
    "type": "equirectangular"
  },
    "mark": {"type": "geoshape"},
    "encoding": {
    "color": {
      "field": "Thousands of Degrees",
      "type": "quantitative"
    }
 }
}
